{"components/common/quick-search":"<li id=\"search\" class=\"desktop-search d-inline-block sideb siden\">\n{{{ snippet \"forms_search\"}}}\n<div class=\"d-search\">\n  <button  class=\"search-toggle\" data-toggle=\"collapse\" onclick=\"openSearch()\" aria-label=\"{{lang \"search.quick_search.input_label\"}}\">\n    \n        <svg height=\"24px\" width=\"24px\"><use xlink:href=\"#mysearch\"></use></svg>\n  </button>\n</div>\n<div id=\"search\" class=\"wbSearch\">\n     <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"wb-search input-group\">\n                <label class=\"is-srOnly\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input form-control\" data-search-quick name=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"\" autocomplete=\"off\">\n                <div class=\"input-group-btn\"><button class=\"wb-search-btn\" align=\"center\" type=\"submit\" value=\"{{lang 'search.results.form_button_text'}}\n                \">search<svg width=\"20px\" height=\"20px\">\n                    <use xlink:href=\"#mysearch\"></use>\n                  </svg>\n                  \n                  <!-- {{lang \"search.quick_search.input_label\"}} -->\n              </button><a href=\"javascript:void(0)\" class=\"closebtn close-nav\" onclick=\"closeSearch()\" aria-label=\"close-nav\"><i class=\"fa fa-close\"></i></a></div>\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n\n</div>\n</li>\n"}
