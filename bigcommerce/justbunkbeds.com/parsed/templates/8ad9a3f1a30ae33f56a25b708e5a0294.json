{"components/common/footer":"{{#if banners.bottom}} {{#if settings.data_tag_enabled}}\r\n<div class=\"banners\" data-banner-location=\"bottom\">\r\n  {{#each (limit banners.bottom_metadata 1)}}\r\n  <div\r\n    class=\"banner\"\r\n    data-event-type=\"promotion\"\r\n    data-entity-id=\"{{this.id}}\"\r\n    data-name=\"{{this.banner-name}}\"\r\n    data-position=\"{{this.location}}\"\r\n    data-name=\"{{this.banner-name}}\">\r\n    <div data-event-type=\"promotion-click\">{{{this.content}}}</div>\r\n  </div>\r\n  {{/each}}\r\n</div>\r\n{{else}}\r\n<div class=\"banners\" data-banner-location=\"bottom\">\r\n  <div class=\"banner\">{{{limit banners.bottom 1}}}</div>\r\n</div>\r\n{{/if}} {{/if}} {{{region name=\"home_above_footer\"}}}\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n  <h2 class=\"footer-title-sr-only\">{{lang 'footer.title'}}</h2>\r\n\r\n  <!-- newsletter section -->\r\n  <div class=\"foot-newsletter fborder\">\r\n    <div class=\"container\">\r\n      <article class=\"footer-info-col\" data-section-type=\"storeInfo\">\r\n        <h3 class=\"footer-info-heading titleWrapper inactive\">\r\n          {{lang 'newsletter.subscribe'}}\r\n        </h3>\r\n        <div id=\"descwrapper\" class=\"desWrapper\">\r\n          <div class=\"webi-subscribe\">\r\n            {{> components/common/subscription-form}}\r\n          </div>\r\n          {{#if theme_settings.footer-store-logo}}\r\n          <ul class=\"list-unstyled foot_img\">\r\n            <li>\r\n              {{#if theme_settings.storeimagefirst}}\r\n              <a href=\"{{theme_settings.storelinkfirst}}\"\r\n                ><img\r\n                  class=\"img-fluid lazyload mx-auto d-block\"\r\n                  data-sizes=\"auto\"\r\n                  width=\"126px\"\r\n                  height=\"44px\"\r\n                  data-src=\"/product_images/uploaded_images/{{theme_settings.storeimagefirst}}\"\r\n                  alt=\"appstore\"\r\n              /></a>\r\n              {{/if}}\r\n            </li>\r\n            <li>\r\n              {{#if theme_settings.storeimagesecond}}\r\n              <a href=\"{{theme_settings.storelinksecond}}\"\r\n                ><img\r\n                  class=\"img-fluid lazyload mx-auto d-block\"\r\n                  data-sizes=\"auto\"\r\n                  width=\"126px\"\r\n                  height=\"44px\"\r\n                  data-src=\"/product_images/uploaded_images/{{theme_settings.storeimagesecond}}\"\r\n                  alt=\"appstore\"\r\n              /></a>\r\n              {{/if}}\r\n            </li>\r\n          </ul>\r\n          {{/if}}\r\n        </div>\r\n      </article>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"container\">\r\n    <div class=\"row\">\r\n      <!-- navigation section -->\r\n      <div class=\"col-lg-3 col-sm-3 col-12 foot-navigate fborder\">\r\n        <article\r\n          class=\"footer-info-col footer-info-col--small\"\r\n          data-section-type=\"footer-webPages\">\r\n          <h3 class=\"footer-info-heading titleWrapper inactive\">\r\n            {{lang 'footer.navigate'}}\r\n            <div class=\"collapse-icon\">\r\n              <div class=\"acc-close\"></div>\r\n              <div class=\"acc-open\"></div>\r\n            </div>\r\n          </h3>\r\n          <div id=\"descwrapper\" class=\"desWrapper\">\r\n            <ul class=\"footer-info-list\">\r\n              {{#each pages}}\r\n              <li>\r\n                <a href=\"{{url}}\">{{name}}</a>\r\n              </li>\r\n              {{/each}}\r\n              <li>\r\n                <a href=\"/sitemap/\">{{lang 'sitemap.page_title'}}</a>\r\n              </li>\r\n               <li>\r\n                <a href=\"/reviews/\">Reviews</a>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </article>\r\n      </div>\r\n\r\n      <!-- payment icon section -->\r\n      <div class=\"col-lg-3 col-sm-3 col-12 foot-payment fborder\">\r\n        <article class=\"footer-info-col footer-info-col--small\">\r\n          <div class=\"cc_imgs\">\r\n            <img\r\n              src=\"https://cdn3.bigcommerce.com/s-rtqvb44/product_images/uploaded_images/icon-creditcards.png\"\r\n              alt=\"credit card logos\" />\r\n          </div>\r\n        </article>\r\n      </div>\r\n\r\n      <div class=\"col-lg-3 col-sm-3 col-12 foot-payment fborder\">\r\n        <article class=\"footer-info-col footer-info-col--small\">\r\n          <div class=\"paypal_imgs for_desktop d-inline-block\">\r\n            <a\r\n              href=\"https://www.paypal.com/us/campaign/terms/paypal-credit\"\r\n              target=\"_blank\">\r\n              <img\r\n                src=\"https://cdn3.bigcommerce.com/s-rtqvb44/product_images/uploaded_images/pay.png\" />\r\n            </a>\r\n          </div>\r\n          <div class=\"seal_imgs for_desktop d-inline-block\">\r\n            <a\r\n              href=\"https://seal.digicert.com/seals/popup/?tag=cDrdpnCH&url=www.justbunkbeds.com&lang=en&cbr=1706764850166\"\r\n              target=\"_blank\">\r\n              <img\r\n                src=\"https://cdn10.bigcommerce.com/s-rtqvb44/images/stencil/original/image-manager/digicert-geotrust-logo.png\" />\r\n            </a>\r\n          </div>\r\n        </article>\r\n      </div>\r\n\r\n      <div class=\"col-lg-3 col-sm-3 col-12 foot-social fborder\">\r\n        <article class=\"footer-info-col footer-info-col--small\">\r\n          <h3 class=\"footer-info-heading titleWrapper inactive\">\r\n            {{lang 'footer.social'}}\r\n            <!-- <div class=\"collapse-icon\">\r\n              <div class=\"acc-close\"></div>\r\n              <div class=\"acc-open\"></div>\r\n            </div> -->\r\n          </h3>\r\n          <ul>\r\n            <li>\r\n              <a\r\n                href=\"https://www.facebook.com/pages/JustBunkBedscom/124722737567869?ref=ts\"\r\n                class=\"icon-social icon-facebook\"\r\n                title=\"Facebook\"\r\n                target=\"_blank\"\r\n                >Facebook</a\r\n              >\r\n            </li>\r\n            <!--<li>\r\n              <a\r\n                href=\"https://twitter.com/JustBunkBeds\"\r\n                class=\"icon-social icon-twitter\"\r\n                title=\"Twitter\"\r\n                Twitter</a\r\n            </li>-->\r\n            <li>\r\n              <a\r\n                href=\"https://twitter.com/JustBunkBeds\"\r\n                class=\"icon-svg icon-svg-x\"\r\n                title=\"Twitter\"\r\n                target=\"_blank\"\r\n                >\r\n                  <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 448 512\"><!--!Font Awesome Free 6.7.2 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2025 Fonticons, Inc.--><path d=\"M64 32C28.7 32 0 60.7 0 96V416c0 35.3 28.7 64 64 64H384c35.3 0 64-28.7 64-64V96c0-35.3-28.7-64-64-64H64zm297.1 84L257.3 234.6 379.4 396H283.8L209 298.1 123.3 396H75.8l111-126.9L69.7 116h98l67.7 89.5L313.6 116h47.5zM323.3 367.6L153.4 142.9H125.1L296.9 367.6h26.3z\"/></svg>\r\n              </a>\r\n            </li>\r\n            <li>\r\n              <a\r\n                href=\"https://www.pinterest.com/justbunkbeds/\"\r\n                class=\"icon-social icon-pinterest\"\r\n                title=\"Pinterest\"\r\n                target=\"_blank\"\r\n                >Pinterest</a\r\n              >\r\n            </li>\r\n            <li>\r\n              <a\r\n                href=\"https://www.instagram.com/justbunkbeds/\"\r\n                class=\"icon-social icon-instagram\"\r\n                title=\"Instagram\"\r\n                target=\"_blank\"\r\n                >Instagram</a\r\n              >\r\n            </li>\r\n            <li>\r\n                <!-- shopper approved seal - templates/components/common/footer.html \r\n                <a href=\"https://www.shopperapproved.com/reviews/www.justbunkbeds.com/\" cla