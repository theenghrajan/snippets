ght-90,r=940;return window.innerWidth<1400&&(r=620),window.open(this.href,\"shopperapproved\",\"location=\"+e+\",scrollbars=yes,width=\"+r+\",height=\"+n+\",menubar=no,toolbar=no\"),o.stopPropagation&&o.stopPropagation(),!1}!function(){for(var o=document.getElementsByClassName(\"shopperlink\"),e=0,n=o.length;e<n;e++)o[e].onclick=openshopperapproved}();'; js.type = \"text/javascript\"; document.getElementsByTagName(\"head\")[0].appendChild(js);var link = document.createElement('link');link.rel = 'stylesheet';link.type = 'text/css';link.href = \"//www.shopperapproved.com/seal/2235.css\";document.getElementsByTagName('head')[0].appendChild(link);})();</script>\r\n\r\n                    <!-- shopper approved seal - templates/components/common/footer.html -->\r\n                    \r\n            </li>\r\n          </ul>\r\n        </article>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"footer-links\">\r\n      <p>\r\n        <span class=\"footer-left\"\r\n          ><span id=\"currencyPrices\"\r\n            >All prices are in <span title=\"US Dollars\">USD</span>.</span\r\n          >\r\n          <span class=\"copyRight\"\r\n            >&copy; {{moment format=\"YYYY\"}} {{settings.store_name}}</span\r\n          >\r\n          <span class=\"pie\">|</span>\r\n          <a href=\"https://www.justbunkbeds.com/sitemap/\"\r\n            ><span>Sitemap</span></a\r\n          >\r\n          <span class=\"pie\">|</span>\r\n        </span>\r\n        <span class=\"powered-by\">BigCommerce</span>\r\n      </p>\r\n    </div>\r\n    \r\n  </div>\r\n\r\n  {{{region name=\"ssl_site_seal--global\"}}}\r\n  <a href=\"\" id=\"scroll\" title=\"Scroll to Top\" style=\"display: block\">\r\n    <i class=\"fa fa-angle-double-up\"></i>\r\n  </a>\r\n</footer>\r\n<!--Taf Interactive Popup-->\r\n\r\n<!--<button class=\"taf-pop-btn\" onclick=\"poptin_display('fd7dc011607c3')\">Get 10% OFF</button>-->\r\n\r\n<!--#Taf Interactive Popup-->\r\n\r\n\r\n<script>\r\n    $( document ).ready(function() {\r\n        console.log('jquery ready');\r\n        $(document).on('click', '.productView-reviewLink', function() {\r\n            $('html, body').animate({\r\n                scrollTop: $(\"#product-reviews\").offset().top - 320}, 1000);\r\n         });\r\n         \r\n         \r\n        /*BOF List Size Sorting*/\r\n         \r\n        var customOrder = [\r\n            \"Twin\", \"Twin XL\", \"Full\", \"Full XL\",\r\n            \"Twin / Full\", \"Twin XL / Full XL\", \"Twin / Queen\",\r\n            \"Twin XL / Queen\", \"Full XL / Queen\", \"Queen\"\r\n        ];\r\n\r\n        var $list = $(\"#facetedSearch-navList--product-size\");\r\n\r\n        if ($list.length === 0) {\r\n            console.warn(\"List not found, retrying...\");\r\n            return;\r\n        }\r\n\r\n        var sortedItems = $list.children(\"li\").toArray().sort(function (a, b) {\r\n            // Extract ONLY the text from the <a> tag, ignoring spans & icons\r\n            var textA = $(a).find(\"a\").contents().filter(function () {\r\n                return this.nodeType === 3; // Get only text nodes\r\n            }).text().trim();\r\n            console.log('textA='+textA);\r\n\r\n            var textB = $(b).find(\"a\").contents().filter(function () {\r\n                return this.nodeType === 3; // Get only text nodes\r\n            }).text().trim();\r\n            console.log('textB='+textB);\r\n\r\n            return customOrder.indexOf(textA) - customOrder.indexOf(textB);\r\n        });\r\n\r\n        $list.empty().append(sortedItems);\r\n        console.log(\"sorted list applied.\");\r\n    \r\n        // Observe changes in the faceted search\r\n        /*var targetNode = document.getElementById(\"facetedSearch-navList--product-size\");\r\n    \r\n        if (targetNode) {\r\n            var observer = new MutationObserver(function () {\r\n                console.log(\"List changed, re-sorting...\");\r\n                setTimeout(sortList, 3000);\r\n            });\r\n    \r\n            observer.observe(targetNode, { childList: true, subtree: true });\r\n        } else {\r\n            console.error(\"list element not found.\");\r\n        }*/\r\n\r\n        /*EOF List Size Sorting*/\r\n        \r\n    });\r\n</script>","components/common/subscription-form":"\n<p>{{theme_settings.subscribe_intro}}</p>\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\"\n                       id=\"nl_name\"\n                       name=\"nl_name\"\n                       type=\"text\"\n                       value=\"{{customer.name}}\"\n                       placeholder=\"{{lang 'newsletter.name_placeholder'}}\"\n                       aria-describedby=\"alertBox-message-text\"\n                       aria-required=\"true\"\n                       required\n                >\n                <input class=\"form-input\"\n                       id=\"nl_email\"\n                       name=\"nl_email\"\n                       type=\"email\"\n                       value=\"{{customer.email}}\"\n                       placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n                       aria-describedby=\"alertBox-message-text\"\n                       aria-required=\"true\"\n                       required\n                >\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\"\n                       type=\"submit\"\n                       value=\"{{lang 'newsletter.subscribe_submit'}}\"\n                >\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n"}
