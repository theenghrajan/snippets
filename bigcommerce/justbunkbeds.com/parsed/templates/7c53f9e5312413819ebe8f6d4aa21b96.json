storeCredit has-dropdown\"\r\n                data-dropdown=\"storeCredit-dropdown\"\r\n                aria-controls=\"storeCredit-dropdown\"\r\n                href=\"{{urls.cart}}\"\r\n                aria-label=\"{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\">\r\n                <span class=\"navUser-action-divider\">\r\n                  {{lang 'common.store_credit_overview'\r\n                  credit=customer.store_credit.formatted}}\r\n                </span>\r\n              </a>\r\n              <ul\r\n                class=\"dropdown-menu\"\r\n                id=\"storeCredit-dropdown\"\r\n                data-dropdown-content\r\n                aria-hidden=\"true\"\r\n                tabindex=\"-1\">\r\n                {{{lang 'common.store_credit'\r\n                store_credit=customer.store_credit.formatted}}}\r\n              </ul>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n        {{/if}} {{#if customer}}\r\n        <li class=\"acd\">\r\n          <a\r\n            class=\"navUser-action dropdown-menu-item sign_out dro\"\r\n            href=\"{{urls.auth.logout}}\"\r\n            ><i class=\"fa fa-sign-out\"></i>{{lang 'common.logout'}}</a\r\n          >\r\n        </li>\r\n        <li class=\"acd account-dropdown\">\r\n          <a\r\n            class=\"navUser-action dropdown-menu-item my_account dro\"\r\n            href=\"{{urls.account.index}}\"\r\n            ><i class=\"fa fa-user\"></i>{{lang 'common.account'}}</a\r\n          >\r\n        </li>\r\n        {{else}}\r\n        <li class=\"mylogin acd\">\r\n          <a\r\n            class=\"navUser-action dropdown-menu-item dro\"\r\n            href=\"{{urls.auth.login}}\"\r\n            ><i class=\"fa fa-lock\"></i>{{lang 'common.login'}}</a\r\n          >\r\n        </li>\r\n        {{#if settings.account_creation_enabled}}\r\n        <li class=\"mycreat acd\">\r\n          <a\r\n            class=\"navUser-action dropdown-menu-item dro\"\r\n            href=\"{{urls.auth.create_account}}\">\r\n            <i class=\"fa fa-user-plus\"></i>{{lang 'common.sign_up'}}\r\n          </a>\r\n        </li>\r\n        {{/if}} {{/if}}\r\n        <li class=\"navUser-item navUser-item-compare acd\">\r\n          <a\r\n            class=\"navUser-action navUser-item--compare\"\r\n            href=\"{{urls.compare}}\"\r\n            data-compare-nav\r\n            aria-label=\"{{lang 'common.compare'}}\"\r\n            ><i class=\"fa fa-compress\"></i> {{lang 'common.compare'}}\r\n            <!-- <span class=\"countPill countPill--positive countPill--alt\"></span> -->\r\n          </a>\r\n        </li>\r\n        <li class=\"navUser-item wi-he acd\">\r\n          <a\r\n            class=\"cart-button cart-button--wishlist dropdown-menu-item navUser-action\"\r\n            title=\"{{lang 'account.nav.wishlist'}}\"\r\n            href=\"{{urls.account.wishlists.all}}\">\r\n            <i class=\"fa fa-heart\"></i>\r\n            {{lang 'wishlist'}}\r\n          </a>\r\n        </li>\r\n        {{#if settings.gift_certificates_enabled}}\r\n        <li class=\"navUser-item acd mygift\">\r\n          <a\r\n            class=\"navUser-action\"\r\n            href=\"{{urls.gift_certificate.purchase}}\"\r\n            aria-label=\"{{lang 'common.gift_cert'}}\"\r\n            ><i class=\"fa fa-gift\"></i>\r\n            {{lang 'common.gift_cert'}}\r\n          </a>\r\n        </li>\r\n        {{/if}} {{#if currency_selector.currencies.length '>' 1}}\r\n        <li class=\"mycurry acd\">\r\n          {{#or customer (unless settings.hide_price_from_guests)}} {{>\r\n          components/common/currency-selector}} {{/or}}\r\n        </li>\r\n        {{/if}}\r\n      </ul>\r\n    </li>\r\n    <!-- cart icon  -->\r\n    <li class=\"navUser-item navUser-item--cart\">\r\n      <a\r\n        class=\"navUser-action\"\r\n        data-cart-preview\r\n        data-dropdown=\"cart-preview-dropdown\"\r\n        data-options=\"align:right\"\r\n        href=\"{{urls.cart}}\"\r\n        aria-label=\"{{lang 'cart.nav_aria_label'}}\"\r\n        ><span class=\"label\">CART</span><svg width=\"24px\" height=\"24px\"><use xlink:href=\"#hcart\"></use></svg>\r\n        <span class=\"cart-icon\"></span>\r\n        <!-- <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span> -->\r\n        <span class=\"countPill cart-quantity\"></span>\r\n      </a>\r\n\r\n      <div\r\n        class=\"dropdown-menu\"\r\n        id=\"cart-preview-dropdown\"\r\n        data-dropdown-content\r\n        aria-hidden=\"true\"></div>\r\n    </li>\r\n  </ul>\r\n  <div class=\"contact-header\">\r\n    <p>\r\n      <a href=\"tel:800-507-5440\" target=\"_blank\"\r\n        >Call us: <strong>800-507-5440</strong></a\r\n      >\r\n    </p>\r\n  </div>\r\n  <div\r\n    class=\"dropdown dropdown--quickSearch\"\r\n    id=\"quickSearch\"\r\n    aria-hidden=\"true\"\r\n    tabindex=\"-1\"\r\n    data-prevent-quick-search-close>\r\n    {{> components/common/quick-search name='nav-quick-search'}}\r\n  </div>\r\n</nav>\r\n","components/common/quick-search":"<li id=\"search\" class=\"desktop-search d-inline-block sideb siden\">\n{{{ snippet \"forms_search\"}}}\n<div class=\"d-search\">\n  <button  class=\"search-toggle\" data-toggle=\"collapse\" onclick=\"openSearch()\" aria-label=\"{{lang \"search.quick_search.input_label\"}}\">\n    \n        <svg height=\"24px\" width=\"24px\"><use xlink:href=\"#mysearch\"></use></svg>\n  </button>\n</div>\n<div id=\"search\" class=\"wbSearch\">\n     <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"wb-search input-group\">\n                <label class=\"is-srOnly\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input form-control\" data-search-quick name=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"\" autocomplete=\"off\">\n                <div class=\"input-group-btn\"><button class=\"wb-search-btn\" align=\"center\" type=\"submit\" value=\"{{lang 'search.results.form_button_text'}}\n                \">search<svg width=\"20px\" height=\"20px\">\n                    <use xlink:href=\"#mysearch\"></use>\n                  </svg>\n                  \n                  <!-- {{lang \"search.quick_search.input_label\"}} -->\n              </button><a href=\"javascript:void(0)\" class=\"closebtn close-nav\" onclick=\"closeSearch()\" aria-label=\"close-nav\"><i class=\"fa fa-close\"></i></a></div>\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n\n</div>\n</li>\n"}
